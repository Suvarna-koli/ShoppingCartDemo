public class HomeController {

	public static String main(String arg[])
	{
		//whenever the user opn our website it shld navigate to home page
		//http://localhost:8080/shoppingcart/
		
		@RequestMapping("/")
		
		public String goToHome()
		{
			return "Home";
		}
	}
}
